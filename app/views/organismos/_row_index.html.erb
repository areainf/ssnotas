
<%
if !@organismos.empty?
    @organismos.each do |organismo|
        @organismo=organismo
        @cssclass=cycle('list-line-odd', 'list-line-even')
%>
<%= render partial: 'single_row_index' %>
  <% end %>
<div class="row">
<div class="tfoot">
<%= ajax_will_paginate @organismos, :id=>"div_pag_ajax", :previous_label=>"&lt;&lt;Ant", :next_label=>"Sig&gt;&gt;", :remote=>"true" %>
</div>
</div>
<%
  else
%>
<div class="row">
  <div  class="col-xs-12 col-sm-12 col-md-12  col-lg-12">
  <h2>No se encontraron resultados</h2>
  </div>
</div>

<% end %>
